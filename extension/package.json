{
	"name": "code-review-assistant",
	"displayName": "Code Review assistant",
	"description": "A smart assistant to help you for your code review",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.codeReview"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.codeReview",
				"title": "Code Review : Activate"
			},
			{
				"command": "extension.codeReview.dialog",
				"title": "Code Review : Dialog"
			  }
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"@types/request-promise": "4.1.44",
		"@types/ws": "6.0.3",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2",
		"request-promise": "4.2.4",
		"request": "2.88.0",
		"botframework-directlinejs": "0.11.4",
		"ws": "7.1.2",
		"xhr2": "0.2.0"
	}
}
